#!/usr/bin/env bash
scons build/NULL/gem5.debug -j4 PROTOCOL=Garnet_standalone 
build/NULL/gem5.debug --outdir=Mesh_XY configs/my/garnet_synth_traffic.py --num-cpus=64 --num-dirs=64 --sys-clock=1.8GHz --ruby-clock=1.8GHz --network=garnet2.0 --topology=Mesh_XY --mesh-rows=8 --sim-cycles=100000 --synthetic=uniform_random --injectionrate=0.5 --vcs-per-vnet=4 && build/NULL/gem5.debug --outdir=NoI_Mesh configs/my/garnet_synth_traffic.py --num-cpus=64 --num-dirs=64 --sys-clock=1.8GHz --ruby-clock=1.8GHz --network=garnet2.0 --topology=NoI_Mesh --mesh-rows=8 --num-chiplets=4 --sim-cycles=100000 --synthetic=uniform_random --injectionrate=0.5 --vcs-per-vnet=4 && build/NULL/gem5.debug --outdir=NoI_CMesh configs/my/garnet_synth_traffic.py --num-cpus=64 --num-dirs=64 --sys-clock=1.8GHz --ruby-clock=1.8GHz --network=garnet2.0 --topology=NoI_CMesh --mesh-rows=8 --num-chiplets=4 --sim-cycles=100000 --synthetic=uniform_random --injectionrate=0.5 --vcs-per-vnet=4
